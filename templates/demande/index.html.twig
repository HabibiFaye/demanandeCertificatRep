{% extends 'base.html.twig' %} {% block title %}Register{% endblock %} {% block body %}
<div class="container">
    <section class="row">
        <div class=" col-10 ">
            <h1 class=" mt-3 mb-3">Entrer vos informations </h1>
            {{ form_errors(form) }} {{ form_start(form) }} {{ form_row(form.Prenom) }} {{ form_row(form.Nom) }} {{ form_row(form.numeroTelephone) }}{{ form_row(form.Lieu_reception_certificat) }}
            <button type="submit" class="btn btn-primary">Register</button>
        </div>

    </section>
</div>

{{ form_end(form) }} {% endblock %}